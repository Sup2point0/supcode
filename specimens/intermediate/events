spec code { ver = 5.3 | syn = utinax | sty = mixed }


on event() {{
  define callback(ctx, (info)s) [(option)s] {
    sys.log((info)s)
  }
}}

on start {{
  define main() {
    sys.out("sup")
  }
}}

on event('test') {{
  sys.out("test")
}}

invoke test


