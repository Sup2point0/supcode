\ times table
\ supcode specimen

spec code { ver = 5.5 | syn = utinax | sty = stan }


create func {
  define 'timesTable'(n) {
    sys.out("  " join(2~n)[" "])

    loop(2~n) {
      set 'x' = loop.count
      set 'line' = {
        loop(2~n) {
          loop.out(loop.count * x)
        }
      }

      sys.out(x " " join(join)[" "])
    }
  }
}