spec code { ver = 5.5 | syn = utinax | sty = stan }


set 'text' = "suppety sup"

sys.out(text)
text#1
text#9~11

case(text)[upper]
case(text)[title]

strip(text)
strip(text)[left]

split(text)
split(text)[white]
split(text)["p"]


set text = "sup", "pety", "sup"

sys.out(join(text))
sys.out(join(text)["-"])
